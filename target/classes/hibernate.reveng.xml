<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="mydb" match-name="Product"/>
  <table-filter match-catalog="mydb" match-name="ProductCategory"/>
  <table-filter match-catalog="mydb" match-name="ProductDetail"/>
  <table-filter match-catalog="mydb" match-name="ProductSpecification"/>
  <table-filter match-catalog="mydb" match-name="ProductSubCategory"/>
  <table-filter match-catalog="mydb" match-name="Project"/>
  <table-filter match-catalog="mydb" match-name="ProjectDetails"/>
  <table-filter match-catalog="mydb" match-name="Specification"/>
  <table-filter match-catalog="mydb" match-name="SubCategorySpecification"/>
  <table-filter match-catalog="mydb" match-name="User"/>
</hibernate-reverse-engineering>